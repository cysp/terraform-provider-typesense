{
    "version": "0.1",
    "provider": {
        "name": "typesense",
        "schema": {
            "attributes": [
                {
                    "name": "url",
                    "string": {
                        "description": "Typesense API URL. Alternatively, can be configured using the `TYPESENSE_URL` environment variable. Alternatively alternatively, can be configured using the `TYPESENSE_PROTOCOL`, `TYPESENSE_HOST` and `TYPESENSE_PORT` environment variables.",
                        "optional_required": "optional"
                    }
                },
                {
                    "name": "api_key",
                    "string": {
                        "description": "Typesense Admin API Key. Alternatively, can be configured using the `TYPESENSE_API_KEY` environment variable.",
                        "optional_required": "optional"
                    }
                }
            ]
        }
    },
    "resources": [
        {
            "name": "key",
            "schema": {
                "attributes": [
                    {
                        "name": "id",
                        "int64": {
                            "computed_optional_required": "computed"
                        }
                    },
                    {
                        "name": "actions",
                        "list": {
                            "computed_optional_required": "required",
                            "element_type": {
                                "string": {}
                            },
                            "plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/listplanmodifier"
                                            }
                                        ],
                                        "schema_definition": "listplanmodifier.RequiresReplace()"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "name": "collections",
                        "list": {
                            "computed_optional_required": "required",
                            "element_type": {
                                "string": {}
                            },
                            "plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/listplanmodifier"
                                            }
                                        ],
                                        "schema_definition": "listplanmodifier.RequiresReplace()"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "name": "description",
                        "string": {
                            "computed_optional_required": "required",
                            "plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/stringplanmodifier"
                                            }
                                        ],
                                        "schema_definition": "stringplanmodifier.RequiresReplace()"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "name": "expires_at",
                        "int64": {
                            "computed_optional_required": "computed_optional",
                            "plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/int64planmodifier"
                                            }
                                        ],
                                        "schema_definition": "int64planmodifier.RequiresReplace()"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "name": "value",
                        "string": {
                            "computed_optional_required": "computed_optional",
                            "plan_modifiers": [
                                {
                                    "custom": {
                                        "imports": [
                                            {
                                                "path": "github.com/hashicorp/terraform-plugin-framework/resource/schema/stringplanmodifier"
                                            }
                                        ],
                                        "schema_definition": "stringplanmodifier.RequiresReplace()"
                                    }
                                }
                            ],
                            "sensitive": true
                        }
                    },
                    {
                        "name": "value_prefix",
                        "string": {
                            "computed_optional_required": "computed"
                        }
                    }
                ],
                "description": "Typesense Key"
            }
        }
    ],
    "datasources": [
        {
            "name": "key",
            "schema": {
                "attributes": [
                    {
                        "name": "id",
                        "int64": {
                            "computed_optional_required": "required"
                        }
                    },
                    {
                        "name": "actions",
                        "list": {
                            "computed_optional_required": "computed",
                            "element_type": {
                                "string": {}
                            }
                        }
                    },
                    {
                        "name": "collections",
                        "list": {
                            "computed_optional_required": "computed",
                            "element_type": {
                                "string": {}
                            }
                        }
                    },
                    {
                        "name": "description",
                        "string": {
                            "computed_optional_required": "computed"
                        }
                    },
                    {
                        "name": "expires_at",
                        "int64": {
                            "computed_optional_required": "computed_optional"
                        }
                    },
                    {
                        "name": "value",
                        "string": {
                            "computed_optional_required": "computed_optional",
                            "sensitive": true
                        }
                    },
                    {
                        "name": "value_prefix",
                        "string": {
                            "computed_optional_required": "computed"
                        }
                    }
                ],
                "description": "Typesense Key"
            }
        }
    ]
}